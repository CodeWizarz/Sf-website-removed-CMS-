    "@radix-ui/react-select": "^1.1.2",
    "@studio-freight/hamo": "^0.4.44",
    "@studio-freight/lenis": "^0.2.26",
    "@studio-freight/tempus": "^0.0.30",
    "blaze-slider": "1.9.0",
    "clsx": "^1.2.1",
    "embla-carousel": "7.0.5",
    "embla-carousel-autoplay": "7.0.5",
    "embla-carousel-react": "7.0.5",
    "graphql": "^16.6.0",
    "graphql-request": "^5.0.0",
    "graphql-tag": "^2.12.6",
    "gsap": "npm:@gsap/business@^3.11.3",
    "lint-staged": "^13.1.0",
    "lottie-web": "^5.10.0",
    "next": "13.0.6",
    "next-pwa": "5.6.0",
    "next-seo": "^5.15.0",
    "next-sitemap": "^3.1.32",
    "next-transpile-modules": "^10.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.40.0",
    "react-use": "^17.4.0",
    "react-use-measure": "^2.1.1",
    "sass": "^1.56.2",
    "throttle-debounce": "5.0.0",
    "zustand": "4.1.5"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.7.3",
    "@next/bundle-analyzer": "^13.0.6",
    "@size-limit/preset-app": "^8.1.0",
    "@svgr/webpack": "^6.5.1",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.8.1",
    "prettier-eslint": "^15.0.1",
    "size-limit": "^8.1.0",
    "stats.js": "^0.17.0"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,scss,md}": "prettier --write"
  },
  "pnpm": {
    "overrides": {
      "throttle-debounce": "5.0.0"
    }
  },
  "size-limit": [
    {
      "path": ".next/static/chunks/**/*.js"
    }
  ]
}
